<%- include('../_layouts/adminheader') %>

<h2 class="page-title">Add a Baby Nursery</h2>
<a href="/admin/categories" class="btn btn-primary">Back to all Nurseries</a>
<br><br>

<form method="post" action="/admin/categories/add-category">
    
    <div class="form-group">
        <label for="">Nursery Title</label>
        <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="Ward Title">
    </div>
    
    <button class="btn btn-default">Submit to Server</button>
</form>

<%- include('../_layouts/adminfooter') %>